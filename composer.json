{
    "name": "cultuurnet/udb3-jwt",
    "description": "Provides JWT service classes",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Bert Ramakers",
            "email": "bert@2dotstwice.be"
        }
    ],
    "require": {
        "cultuurnet/clock": "~0.1",
        "lcobucci/jwt": "~3.1",
        "cultuurnet/valueobjects": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7",
        "silex/silex": "^1.3"
    },
    "suggest": {
        "silex/silex": "To use the JwtServiceProvider"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3\\Jwt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3\\Jwt\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
